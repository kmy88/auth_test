# 데이터베이스 설정
DATABASE_URL=sqlite:///auth_tool.db

# 암호화 마스터 키 (선택사항 - 비워두면 자동 생성됨)
MASTER_KEY=

# API 서버 설정
API_HOST=127.0.0.1
API_PORT=8000

# CORS 설정 (쉼표로 구분)
CORS_ORIGINS=*

# 암호화 키 파일 경로
ENCRYPTION_KEY_FILE=encryption.key

# 로그 레벨
LOG_LEVEL=INFO

# 환경 설정 (development/production)
ENVIRONMENT=development